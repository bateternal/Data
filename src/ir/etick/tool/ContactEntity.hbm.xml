<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="ir.etick.tool.ContactEntity" table="contact" schema="etick">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
        </id>
        <property name="melliCode">
            <column name="melli_code" sql-type="varchar(10)" length="10" not-null="true"/>
        </property>
        <property name="firstName">
            <column name="first_name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
        <property name="lastName">
            <column name="last_name" sql-type="varchar(30)" length="30" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>